jQuery(function($) {
    /**
     * Slick options
     */

    $('.contributor_slider').slick({
        infinite: true,
        speed: 700,
        autoplay:true,
        autoplaySpeed: 2000,
        arrows:true,
        slidesToShow: 3,
        slidesToScroll: 1,
        responsive: [
        {
            breakpoint: 780,
            settings: {
                arrows:true,
                slidesToShow: 1,
                slidesToScroll: 1
            }
        },
        {
            breakpoint: 480,
            settings: {
                arrows:true,
                slidesToShow: 1,
                slidesToScroll: 1
            }
        }
    ]
    });

});